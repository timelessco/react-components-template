{
  "name": "react-components-template",
  "version": "1.0.0",
  "description": "React Components Project template",
  "keywords": [
    "react",
    "components",
    "template",
    "boilerplate",
    "typescript",
    "new-project",
    "new-repo"
  ],
  "bugs": {
    "url": "https://github.com/timelessco/react-components-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timelessco/react-components-template.git"
  },
  "license": "MIT",
  "author": "Navin Moorthy <navin@timeless.co>",
  "scripts": {
    "build": "react-scripts build",
    "build-storybook": "build-storybook",
    "commit": "gacp",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,ts,css,less,json,md,html,yml,yaml,pcss,jsx,tsx}\"",
    "format:package": "sort-package-json",
    "lint": "eslint src --ext .tsx,.ts,.jsx,.js",
    "storybook": "start-storybook -p 6006"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "gacp": "^2.10.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sort-package-json": "^1.48.0",
    "typescript": "^4.1.2"
  },
  "peer-dependencies": {
    "react": "16.x || 17.x",
    "react-dom": "16.x || 17.x"
  }
}
